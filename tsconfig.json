{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noLib": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2019"
  }
}
