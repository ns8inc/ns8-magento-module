  "license": "MIT",
  "main": "dist/switchboard.min.js",
  "name": "@ns8/protect-integration-magento",
  "private": true,
  "scripts": {
    "beautify": "ts-node -P ./build/tsconfig.json ./build/projectBuild.ts",
    "build": "yarn && yarn generate:exports && yarn beautify && tsc --emitDeclarationOnly && yarn bundle && yarn module:config",
    "build:dev": "NODE_ENV=dev yarn build",
    "build:prod": "NODE_ENV=prod yarn build",
    "bundle": "webpack",
    "clean": "rm -rf .awcache dist .serverless",
    "deploy": "METHOD=deploy ts-node -P ./build/tsconfig.json ./build/lambdaDeploy.ts",
    "generate:exports": "cti create ./switchboard -w",
    "lint": "eslint --ext=ts switchboard build",
    "lint:fix": "yarn lint --fix",
    "lint:markdown": "eslint --ext md .",
    "magento:setversion": "ts-node build/setMagentoVersion.ts",
    "module:build": "cd module && composer install",
    "module:config": "ts-node -P ./build/tsconfig.json ./build/moduleConfig.ts",
    "module:deploy": "yarn module:config && ts-node -P ./build/tsconfig.json ./build/moduleDeploy.ts",
    "module:lint": "cd module && phpcs --standard=Magento2 --ignore='*/vendor/*,*.min.js' ./",
    "module:release": "NODE_ENV=prod && yarn module:config && ts-node -P ./build/tsconfig.json ./build/moduleRelease.ts",
    "rebuild": "yarn clean && yarn build",
    "rebuild:dev": "yarn clean && yarn build:dev",
    "test": "nyc mocha 'test/**/*Test.ts'",
    "test:debug": "mocha -r ts-node/register 'test/**/*Test.ts'",
    "undeploy": "METHOD=remove ts-node -P ./build/tsconfig.json ./build/lambdaDeploy.ts",
    "version:patch": "ts-node -P ./build/tsconfig.json ./build/incrementVersion.ts && yarn build && yarn publish",
    "version:patch:dev": "PATCH_MODE=dev yarn version:patch",
    "version:publish": "yarn && yarn build && npm publish"
  },
  "types": "dist/types/index.d.ts",
  "version": "878792"
}
