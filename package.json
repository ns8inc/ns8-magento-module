{
  "author": "ns8inc",
  "dependencies": {},
  "description": "magento extension project",
  "devDependencies": {
    "@ns8/magento2-rest-client": "0.2.28",
    "@ns8/ns8-protect-sdk": "0.1.19",
    "@ns8/ns8-switchboard-operator": "1.0.102",
    "@types/archiver": "3.0.0",
    "@types/node": "12.12.3",
    "@types/rimraf": "2.0.3",
    "@types/semver": "6.2.0",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "archiver": "3.1.1",
    "aws-lambda": "0.1.2",
    "aws-sdk": "2.560.0",
    "dotenv-extended": "2.5.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "isomorphic-fetch": "2.2.1",
    "minimist": "1.2.0",
    "ns8-protect-models": "1.0.211",
    "ns8-switchboard-interfaces": "1.0.87",
    "rimraf": "3.0.0",
    "scp2": "0.5.0",
    "semver": "6.3.0",
    "serverless": "1.56.1",
    "ts-node": "8.4.1",
    "typescript": "3.6.4"
  },
  "license": "MIT",
  "name": "ns8-magento-platform",
  "scripts": {
    "beautify": "ts-node ./build/projectBuild.ts",
    "build": "yarn && yarn compile && yarn beautify && yarn module:config",
    "build:dev": "set NODE_ENV=dev yarn build && yarn link",
    "build:prod": "set NODE_ENV=prod yarn build",
    "clean": "rimraf dist",
    "compile": "tsc -d",
    "lint": "eslint --ext=ts switchboard switches",
    "module:build": "cd module && composer install",
    "module:config": "ts-node ./build/moduleConfig.ts",
    "module:deploy": "yarn module:config && ts-node ./build/moduleDeploy.ts",
    "module:lint": "cd module && phpcs --standard=Magento2 --ignore=*/vendor/*,*.min.js ./",
    "module:release": "set NODE_ENV=prod && yarn module:config && ts-node ./build/moduleRelease.ts",
    "switchboard:build": "cd switchboard && yarn build",
    "switches:build": "cd switches && yarn build",
    "version:inc": "ts-node ./build/incrementVersion.ts"
  },
  "version": "2.0.0"
}
