    "scp2": "0.5.0",
    "semver": "7.1.1",
    "serverless": "1.61.3",
    "serverless-pseudo-parameters": "2.5.0",
    "serverless-step-functions": "2.17.0",
    "ts-mocha": "6.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5",
    "url": "0.11.0",
    "uuid": "3.4.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-merge": "4.2.2"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/switchboard.min.js",
  "name": "@ns8/ns8-magento-platform",
  "scripts": {
    "beautify": "ts-node --skip-project ./build/projectBuild.ts",
    "build": "yarn && yarn generate:exports && yarn beautify && yarn bundle && yarn module:config",
    "build:dev": "NODE_ENV=dev tsc -d --sourceMap && yarn build",
    "build:prod": "NODE_ENV=prod tsc -d && yarn build",
    "bundle": "webpack",
    "clean": "rm -rf .awcache .tmp dist .serverless",
    "deploy": "METHOD=deploy && ts-node --skip-project ./build/lambdaDeploy.ts",
    "generate:exports": "yarn cti create ./switchboard -w",
    "lint": "eslint --ext=ts switchboard",
    "lint:fix": "yarn lint --fix",
    "module:build": "cd module && composer install",
    "module:config": "ts-node --skip-project ./build/moduleConfig.ts",
    "module:deploy": "yarn module:config && ts-node --skip-project ./build/moduleDeploy.ts",
    "module:lint": "cd module && phpcs --standard=Magento2 --ignore='*/vendor/*,*.min.js' ./",
    "module:release": "NODE_ENV=prod && yarn module:config && ts-node --skip-project ./build/moduleRelease.ts",
    "rebuild": "yarn clean && yarn build",
    "rebuild:dev": "yarn clean && yarn build:dev",
    "test": "yarn build && nyc mocha 'test/**/*Test.ts'",
    "test:debug": "mocha -r ts-node/register 'test/**/*Test.ts'",
    "undeploy": "METHOD=remove && ts-node --skip-project ./build/lambdaDeploy.ts",
    "version:patch": "ts-node --skip-project ./build/incrementVersion.ts && yarn publish && yarn build",
    "version:patch:dev": "PATCH_MODE=dev && yarn version:patch",
    "version:publish": "yarn && yarn build && npm publish"
  },
  "version": "2.0.41"
}
