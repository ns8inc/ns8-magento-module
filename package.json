{
  "author": "ns8inc",
  "dependencies": {},
  "description": "magento extension project",
  "devDependencies": {
    "@ns8/magento2-rest-client": "0.2.29",
    "@ns8/ns8-protect-sdk": "0.1.19",
    "@ns8/ns8-switchboard-operator": "1.0.103",
    "@types/archiver": "3.0.0",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.14",
    "@types/rimraf": "2.0.3",
    "@types/semver": "6.2.0",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "archiver": "3.1.1",
    "aws-lambda": "0.1.2",
    "aws-sdk": "2.584.0",
    "cross-env": "6.0.3",
    "dotenv-extended": "2.5.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "ns8-protect-models": "1.0.211",
    "ns8-switchboard-interfaces": "1.0.87",
    "rimraf": "3.0.0",
    "scp2": "0.5.0",
    "semver": "6.3.0",
    "serverless": "1.56.1",
    "ts-node": "8.5.4",
    "typescript": "3.7.3",
    "webpack": "4.41.2"
  },
  "license": "MIT",
  "name": "ns8-magento-platform",
  "scripts": {
    "beautify": "ts-node ./build/projectBuild.ts",
    "build": "yarn build:root && yarn switches:build && yarn switchboard:build",
    "build:dev": "cross-env NODE_ENV=dev yarn build:root && yarn switches:build:dev && yarn switchboard:build:dev",
    "build:prod": "cross-env NODE_ENV=prod yarn build",
    "build:root": "yarn && yarn compile && yarn beautify && yarn module:config",
    "certbot": "./scripts/ssl_cert.sh",
    "certbot:prep": "./scripts/ssl_cert_prep.sh",
    "clean": "rimraf dist",
    "compile": "tsc -d",
    "lint": "eslint --ext=ts switchboard switches",
    "module:build": "cd module && composer install",
    "module:config": "ts-node ./build/moduleConfig.ts",
    "module:deploy": "yarn module:config && ts-node ./build/moduleDeploy.ts",
    "module:lint": "cd module && phpcs --standard=Magento2 --ignore=*/vendor/*,*.min.js ./",
    "module:release": "cross-env NODE_ENV=prod && yarn module:config && ts-node ./build/moduleRelease.ts",
    "switchboard:build": "cd switchboard && yarn && yarn build",
    "switchboard:build:dev": "cd switchboard && yarn && yarn build:dev",
    "switchboard:deploy": "cross-env METHOD=deploy && ts-node ./build/lambdaDeploy.ts",
    "switchboard:undeploy": "cross-env METHOD=remove && ts-node ./build/lambdaDeploy.ts",
    "switches:build": "cd switches && yarn && yarn build",
    "switches:build:dev": "cd switches && yarn && yarn build:dev",
    "switches:publish": "cd switches && yarn && yarn build && npm publish",
    "version:patch": "ts-node ./build/incrementVersion.ts && yarn switches:publish && yarn build",
    "version:patch:dev": "cross-env PATCH_MODE=dev && yarn version:patch"
  },
  "version": "2.0.27"
}
