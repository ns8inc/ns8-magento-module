<?xml version="1.0"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
  <route url="/V1/Protect/score/:orderId/:eq8"
    method="POST">
    <service class="NS8\Protect\Api\OrderScoreInterface"
      method="score" />
    <resources>
      <resource ref="NS8_Protect::admin" />
    </resources>
  </route>
</routes>
