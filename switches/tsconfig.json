{
  "compilerOptions": {
    "incremental": false,
    "module": "commonjs",
    "outDir": "dist",
    "target": "ES2017",
    "esModuleInterop": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "exclude": [
    "test/**/*",
    "node_modules",
    "dist"
  ],
  "include": [
    "src/**/*",
    "test/magento/MagentoClientTest.js"
  ]
}
