{
  "dependencies": {
    "@ns8/ns8-protect-sdk": "^0.1.10",
    "@ns8/ns8-switchboard-operator": "^1.0.95",
    "@types/node": "^12.7.5",
    "aws-lambda": "^0.1.2",
    "lodash": "^4.17.15",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-step-functions": "^1.25.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  },
  "optionalDependencies": {
    "@types/chai": "^4.2.2",
    "@types/lodash": "^4.14.141",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13"
  },
  "scripts": {
    "build": "yarn compile && yarn install-modules",
    "build:debug": "yarn && yarn build && yarn link @ns8/ns8-magento2-switches && yarn install-modules:debug",
    "build:dev": "yarn build && yarn link @ns8/ns8-magento2-switches",
    "compile": "rimraf dist && yarn install && tsc -d",
    "deploy": "sls deploy",
    "install-modules": "node dist/src/init.js",
    "install-modules:debug": "node --inspect-brk=9228 dist/src/init.js",
    "undeploy": "sls remove"
  }
